<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="esuquillo_sem6.insertar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="FORMULARIO PARA INSERTAR " HorizontalTextAlignment="Center"  FontSize="22"  />
            <Entry x:Name="txtCodigo" Placeholder="Codigo" Keyboard="Numeric"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre" />
            <Entry x:Name="txtApelldio" Placeholder="Apelliod" />
            <Entry x:Name="txtEdad" Placeholder="Edad" Keyboard="Numeric"/>

            <Button x:Name="btnInser" Text="INSERTAR" Clicked="btnInser_Clicked" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>